{
  "name": "resume-site",
  "version": "1.0.0",
  "description": "Resume website with Handlebars and i18n",
  "scripts": {
    "build": "tsc && node dist/build.js",
    "watch": "tsc && node dist/build.js --watch",
    "dev": "npm run build && concurrently \"npm run watch\" \"live-server dist --port=3434 --open=/index.html --wait=500\"",
    "dev:build": "ts-node src/build.ts -- --watch"
  },
  "keywords": [
    "resume",
    "handlebars",
    "i18n",
    "static"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.11.19",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.0",
    "@types/node": "^20.10.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
